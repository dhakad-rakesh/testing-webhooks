<table class="table table-striped table-bordered table-hover">
  <thead>
    <tr>
      <th><%= t('user_name') %></th>
      <th><%= t('game_name') %></th>
      <th><%= t('validity_days') %></th>
      <th><%= t('actions') %></th>
    </tr>
  </thead>
  <tbody>
    <% rounds.each do |rd| %>
      <tr class="gradeX">
        <td><%= rd&.user&.username %></td>
        <td><%= rd&.q_tech_casino_game&.name %></td>
        <td><%= rd&.validity_days %></td>
        <td>
          <%= link_to admin_q_tech_free_round_path(rd),
              method: :delete, data: { confirm: t('data_confirm_dialog') }, class: 'btn btn-primary' do %>
            <span class="fa fa-trash"></span>
          <% end %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= will_paginate @rounds, renderer: BootstrapPagination::Rails %>
