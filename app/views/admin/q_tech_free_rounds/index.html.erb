<div class="row wrapper border-bottom white-bg page-heading">
  <div class="col-lg-10">
    <h2><%= t('round') %></h2>
  </div>
  <div class="col-lg-2">
    <div class="back-btn">
      <%= link_to :back, class: 'btn btn-primary' do %>
        <span class="fa fa-arrow-circle-o-left"></span> Back
      <% end %>
    </div>
  </div>
</div>
<div class="wrapper wrapper-content animated fadeInRight">

  <div class="row">
    <div class="col-lg-12">
      <div class="ibox float-e-margins">
        <div class="ibox-title">
          <h5>Free Round Spin List</h5>
          <div class="ibox-tools">
            <%= link_to "Add Free Round Spin", new_admin_q_tech_free_round_path, class: "btn btn-primary btn-sm" %>
          </div>
        </div>
        <div class="ibox-content">
          <div class="table-responsive">
            <%= render 'admin/q_tech_free_rounds/rounds_list', rounds: @rounds %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
