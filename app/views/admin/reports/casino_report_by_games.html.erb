<div class="row wrapper border-bottom white-bg page-heading">
  <div class="col-lg-10">
    <h2><%= t('casino_reports_by_games') %></h2>
  </div>
  <div class="col-lg-2">
    <div class="back-btn">
      <%= link_to :back, class: 'btn btn-primary' do %>
        <span class="fa fa-arrow-circle-o-left"></span> Back
      <% end %>
    </div>
  </div>
</div>

<div class="wrapper wrapper-content animated fadeInRight">
  <div class="row">
    <%= render "casino_report_by_game_filter" %>
  </div>
  
  <div class="row">
    <div class="col-lg-12">
      <div class="ibox float-e-margins">
        <div class="ibox-title">
          <h5>Report</h5>
          <div class="ibox-tools">
          </div>
        </div>
        <div class="ibox-content" id="casino-game-report-list">
          <%= render 'casino_report_by_game', games: @games %>
        </div>
      </div>
    </div>
  </div>
</div>
